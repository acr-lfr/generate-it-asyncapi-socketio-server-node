import { SocketJWT } from '@/interfaces/SocketJWT';
{% set channel = operations[0].channel.publish -%}
import { {{ channel['x-response-definitions'] }} } from '@/generated/interfaces';

export default (
  routingKey: string,
  payload: {{ channel['x-response-definitions'] }},
  socket: SocketJWT
): void => {
  if (payload.toActor.username === socket.decoded_token.username) {
    socket.emit(routingKey, payload);
  }
}
